[
  {
    "Resource": "arn:aws:s3:::bucket",
    "Policy": {
      "Version": "2012-10-17",
      "Id": "ImplicitDenyAlmostAll",
      "Statement": [
        {
          "Sid": "AllowNonExistentPrincipal",
          "Effect": "Allow",
          "Principal": "*",
          "Action": "s3:GetObject",
          "Resource": [
            "arn:aws:s3:::bucket",
            "arn:aws:s3:::bucket/*"
          ],
          "Condition": {
            "ArnEquals": {
              "aws:PrincipalArn": [
                "arn:aws:iam::111111111111:user/some-other-user"
              ]
            }
          }
        }
      ]
    },
    "Account": "111111111111"
  }
]